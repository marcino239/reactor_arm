<?xml version="1.0"?>
<robot name="reactor_arm" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find reactor_description)/urdf/reactor.gazebo" />
  <!-- Import Rviz colors -->
  <xacro:include filename="$(find reactor_description)/urdf/materials.xacro" />

  <xacro:property name="PI" value="3.1415926535897931"/>
  
    <!-- Used for fixing robot to Gazebo 'base_link' -->
  <link name="world"/>

  <joint name="fixed" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
  </joint>

  <link
    name="base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia
	  ixx="1.0" ixy="0.0" ixz="0.0"
	  iyy="1.0" iyz="0.0"
	  izz="1.0"/>
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/base_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.25098 0.25098 0.25098 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/base_link_hull.STL" />
      </geometry>
    </collision>
  </link>
 
  <link
    name="shoulder_link">
    <inertial>
      <origin
        xyz="5.5296E-07 0.00048339 0.0026355"
        rpy="0 0 0" />
      <mass
        value="0.2" />
      <inertia
	  ixx="0.1" ixy="0.0" ixz="0.0"
	  iyy="0.1" iyz="0.0"
	  izz="0.1"/>
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/shoulder_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.25098 0.25098 0.25098 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/shoulder_link_hull.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="shoulder_yaw"
    type="revolute">
    <origin
      xyz="0 0 0.081"
      rpy="0 0 0" />
    <parent
      link="base_link" />
    <child
      link="shoulder_link" />
    <axis
      xyz="0 0 1" />
    <limit
      effort="10"
      velocity="6.178465545" 
      lower="-3.1"
      upper="3.1" />
  </joint>
  <link
    name="bicep_link">
    <inertial>
      <origin
        xyz="-0.079312 0.00175 0.0073486"
        rpy="0 0 0" />
      <mass
        value="0.2" />
      <inertia
	  ixx="0.1" ixy="0.0" ixz="0.0"
	  iyy="0.1" iyz="0.0"
	  izz="0.1"/>
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/bicep_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.32157 0.32157 0.32157 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/bicep_link_hull.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="shoulder_pitch"
    type="revolute">
    <origin
      xyz="0 0 0.0265"
      rpy="0 0 0" />
    <parent
      link="shoulder_link" />
    <child
      link="bicep_link" />
    <axis
      xyz="0 1 0" />
    <limit
      effort="15"
      velocity="6.178465545" 
      lower="0"
      upper="3.141593" />
  </joint>
    <link
    name="forearm_link">
    <inertial>
      <origin
        xyz="0.0851888339312843 0.00136288621406396 -0.000141576344087929"
        rpy="0 0 0" />
      <mass
        value="0.2" />
      <inertia
	  ixx="0.1" ixy="0.0" ixz="0.0"
	  iyy="0.1" iyz="0.0"
	  izz="0.1"/>
    </inertial>

    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/forearm_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.250980392156863 0.250980392156863 0.250980392156863 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/forearm_link_hull.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="elbow_pitch"
    type="revolute">
    <origin
      xyz="-0.1445 0 0.0385"
      rpy="0 0 0" />
    <parent
      link="bicep_link" />
    <child
      link="forearm_link" />
    <axis
      xyz="0 -1 0" />
    <limit
      effort="20"
      velocity="6.178465545" 
      lower="0"
      upper="3.141593" />
  </joint>
    <link
    name="wrist_1">
    <inertial>
      <origin
        xyz="0.0426453655090486 0.0017494960392062 0.00984847105297723"
        rpy="0 0 0" />
      <mass
        value="0.2" />
      <inertia
	  ixx="0.1" ixy="0.0" ixz="0.0"
	  iyy="0.1" iyz="0.0"
	  izz="0.1"/>
    </inertial>

    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/wrist_1.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.250980392156863 0.250980392156863 0.250980392156863 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/wrist_1_hull.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="wrist_pitch"
    type="revolute">
    <origin
      xyz="0.1535 0 0"
      rpy="0 0 0" />
    <parent
      link="forearm_link" />
    <child
      link="wrist_1" />
    <axis
      xyz="0 1 0" />
    <limit
      effort="10"
      velocity="6.178465545" 
      lower="-1.57079632679"
      upper="1.57079632679" />
  </joint>
    <link
    name="wrist_2">
    <inertial>
      <origin
        xyz="0.022349 -5.4754E-07 0.0089164"
        rpy="0 0 0" />
      <mass
        value="0.2" />
      <inertia
	  ixx="0.1" ixy="0.0" ixz="0.0"
	  iyy="0.1" iyz="0.0"
	  izz="0.1"/>
    </inertial>

    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/wrist_2.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.32157 0.32157 0.32157 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/wrist_2_hull.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="wrist_roll"
    type="revolute">
    <origin
      xyz="0.071 0.00175 0"
      rpy="0 0 0" />
    <parent
      link="wrist_1" />
    <child
      link="wrist_2" />
    <axis
      xyz="1 0 0" />
    <limit
      effort="10"
      velocity="6.178465545" 
      lower="-3.1"
      upper="3.1" />
  </joint>
  <link
    name="gripper_link">
    <inertial>
      <origin
        xyz="0.0091884 -7.3309E-13 -3.4513E-05"
        rpy="0 0 0" />
      <mass
        value="0.2" />
      <inertia
	  ixx="0.1" ixy="0.0" ixz="0.0"
	  iyy="0.1" iyz="0.0"
	  izz="0.1"/>
    </inertial>

    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/gripper_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.25098 0.25098 0.25098 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://reactor_description/meshes/gripper_link_hull.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="gripper_joint"
    type="fixed">
    <origin
      xyz="0.04789 0 0"
      rpy="0 0 0" />
    <parent
      link="wrist_2" />
    <child
      link="gripper_link" />
    <axis
      xyz="1 0 0" />
    <limit
      effort="10"
      velocity="6.178465545" 
      lower="-3.1"
      upper="3.1" />
  </joint>
  
  <transmission name="shoulder_yaw_tran">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="shoulder_yaw">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor1">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="shoulder_pitch_tran">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="shoulder_pitch">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor2">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="elbow_pitch_tran">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="elbow_pitch">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor3">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
